<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Border (stroke) -->
    <stroke
        android:width="2dp"
        android:color="#C9DABF"/>

    <!-- Background color (optional) -->
    <solid android:color="@color/white"/> <!-- warna latar belakang putih -->

    <corners android:radius="15dp"/>

    <!-- Padding (optional) -->
    <padding
        android:left="8dp"
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp"/>
</shape>
